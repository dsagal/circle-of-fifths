{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "strictPropertyInitialization": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "plugins": [{
      "name": "typescript-tslint-plugin"
    }],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "outDir": "build/"
  },
}
